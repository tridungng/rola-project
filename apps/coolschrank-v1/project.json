{
  "name": "coolschrank-v1",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "generators": {},
  "sourceRoot": "apps/coolschrank-v1/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "outputPath": "dist/apps/coolschrank-v1",
        "index": "apps/coolschrank-v1/src/index.html",
        "main": "apps/coolschrank-v1/src/main.ts",
        "polyfills": "apps/coolschrank-v1/src/polyfills.ts",
        "tsConfig": "apps/coolschrank-v1/tsconfig.app.json",
        "assets": [
          "apps/coolschrank-v1/src/favicon.ico",
          "apps/coolschrank-v1/src/assets"
        ],
        "styles": [
          "apps/coolschrank-v1/src/styles.css",
          "node_modules/bootstrap/dist/css/bootstrap.min.css"
        ],
        "scripts": ["node_modules/bootstrap/dist/js/bootstrap.min.js"],
        "vendorChunk": true,
        "extractLicenses": false,
        "buildOptimizer": false,
        "sourceMap": true,
        "optimization": false,
        "namedChunks": true
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/coolschrank-v1/src/environments/environment.ts",
              "with": "apps/coolschrank-v1/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": true,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "6kb",
              "maximumError": "10kb"
            }
          ]
        }
      },
      "defaultConfiguration": ""
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "options": {
        "browserTarget": "coolschrank-v1:build"
      },
      "configurations": {
        "production": {
          "browserTarget": "coolschrank-v1:build:production"
        }
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "coolschrank-v1:build"
      }
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "main": "apps/coolschrank-v1/src/test.ts",
        "polyfills": "apps/coolschrank-v1/src/polyfills.ts",
        "tsConfig": "apps/coolschrank-v1/tsconfig.spec.json",
        "karmaConfig": "apps/coolschrank-v1/karma.conf.js",
        "assets": [
          "apps/coolschrank-v1/src/favicon.ico",
          "apps/coolschrank-v1/src/assets"
        ],
        "styles": ["apps/coolschrank-v1/src/styles.css"],
        "scripts": []
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/coolschrank-v1/**/*.ts",
          "apps/coolschrank-v1/**/*.html"
        ]
      }
    }
  }
}
